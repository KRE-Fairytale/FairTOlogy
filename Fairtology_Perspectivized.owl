        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Lens</IRI>
        <Literal>The lens used by perspectivisation on an eventuality, e.g., &apos;investments&apos; on current taxation policies.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty IRI="http://purl.org/dc/elements/1.1/creator"/>
        <IRI>http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl#Perspectivisation</IRI>
